<%# Local variables: ga_token [String] google analytics authentication token
                     hub [String] hub name
                     contributor [String] contributor name (optional) %>
      
<!-- Data for google analytics javascript embed api -->
<div id="access-token" data-access-token="<%= ga_token %>" />
<div id="profile-id" data-profile-id="<%= Settings.google_analytics.frontend_profile_id %>" />
<div id="hub" data-hub="<%= hub %>" />
<% contributor ||= "" %>
<div id="contributor" data-contributor="<%= contributor %>" />
<div id="start-date" data-start-date="<%= @start_date.iso8601 %>" />
<div id="end-date" data-end-date="<%= @end_date.iso8601 %>" />


<h2>DPLA website use timelines</h2>

<div class="description-text">
  This feature is no longer available due to new security protocols. It may be restored in the future.
</div>

<% if false %>

<div class="description-text">
  <%= website_use_tooltip %>
  <%= timelines_tooltip %>
</div>

<br/>

<h3>Item views</h3>
<div class="description-text"><%= view_item_tooltip %></div>
<section id="item-view-timeline">
  If timeline does not load after a few seconds, try refreshing the page.
</section>

<h3>Click throughs</h3>
<div class="description-text"><%= click_through_tooltip %></div>
<section id="click-through-timeline">
  If timeline does not load after a few seconds, try refreshing the page.
</section>

<h3>Users</h3>
<div class="description-text"><%= users_tooltip %></div>
<section id="user-timeline">
  If timeline does not load after a few seconds, try refreshing the page.
</section>

<h3>Sessions</h3>
<div class="description-text"><%= sessions_tooltip %></div>
<section id="session-timeline">
  If timeline does not load after a few seconds, try refreshing the page.
</section>

<% end %>
