<% content_for :head_script do %>
  <%= javascript_include_tag 'google-analytics', 'data-turbolinks-track': 'reload', defer: 'defer' %>
<% end %>

<h1><%= @contributor.name %></h1>

<%= render partial: "shared/use_metrics", locals: {target: @contributor} %>

<%= render partial: "shared/map", locals: { ga_token: @contributor.ga_token,
  hub: @contributor.hub, contributor: @contributor.name } %>
